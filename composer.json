{
    "name": "svr/raw",
    "description": "SVR RAW - A simple Laravel package.",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/Spirit412/svr_raw",
    "authors": [
        {
            "name": "Domanskiy Aleksandr",
            "email": "domanskiy.av@plinor.ru"
        }
